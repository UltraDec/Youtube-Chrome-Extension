{
    "manifest_version": 3,
    "name": "UltraDec's Youtube Tool",
    "version": "0.3",
    "description": "An extension which automatically sets playback speed of youtube videos. Music will play at a separate speed to other videos.",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}